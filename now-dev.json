{
  "version": 2,
  "builds": [
    { "src": "studio/public/**", "use": "@now/static" },
    { "src": "www/package.json", "use": "@now/static-build" },
    { "src": "www/src/server/*.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/www/favicon.ico" },
    { "src": "/static/(.*)", "dest": "/www/$1" },
    { "src": "/studio/static/(.*)", "dest": "/studio/public/static/$1" },
    { "src": "/studio", "dest": "/studio/public/index.html" },
    { "src": "/studio/.*", "dest": "/studio/public/index.html" },
    { "src": "/", "dest": "/www/src/server/ssr.ts" },
    { "src": "/.*", "dest": "/www/src/server/notFound.ts" }
  ]
}
