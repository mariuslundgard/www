{
  "version": 2,
  "builds": [
    { "src": "static/**", "use": "@now/static" },
    { "src": "src/*.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/static/favicon.ico" },
    { "handle": "filesystem" },
    { "src": "/", "dest": "/src/ssr.ts" },
    { "src": "/.*", "dest": "/src/notFound.ts" }
  ]
}
