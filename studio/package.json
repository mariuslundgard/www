    "@sanity/ui": "^0.36.15",
    "@sanity/vision": "2.21.10",
    "codemirror": "^5.63.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
